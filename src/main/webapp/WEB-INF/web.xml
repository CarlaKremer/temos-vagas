<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>ServCadEmpresa</servlet-name>
        <servlet-class>Control.ServCadEmpresa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServEntrar</servlet-name>
        <servlet-class>Control.ServEntrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUploadDBServlet</servlet-name>
        <servlet-class>DAO.FileUploadDBServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUploadDBServlete</servlet-name>
        <servlet-class>Control.FileUploadDBServlete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Control.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServLogin</servlet-name>
        <servlet-class>Control.ServLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servTeste</servlet-name>
        <servlet-class>servTeste</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServCadEmpresa</servlet-name>
        <url-pattern>/servCadEmpresa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServEntrar</servlet-name>
        <url-pattern>/servEntrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploadDBServlet</servlet-name>
        <url-pattern>/fileUploadDBServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploadDBServlete</servlet-name>
        <url-pattern>/fileUploadDBServlete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServLogin</servlet-name>
        <url-pattern>/ServLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servTeste</servlet-name>
        <url-pattern>/servTeste</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
